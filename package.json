{
  "name": "development-exercise-master",
  "version": "1.0.0",
  "description":
    "For this excercise you are tasked with recreating this article page with one interactive element, an editable title.",
  "main": "index.js",
  "scripts": {
    "build-css":
      "node-sass-chokidar --include-path ./exercise/public/styles exercise/public/styles -o exercise/public",
    "watch-css":
      "npm run build-css && node-sass-chokidar --include-path ./exercise/public/styles exercise/public/styles -o exercise/public --watch --recursive",
    "http-serve": "http-server exercise -p 3000 -c10 -g",
    "server": "node exercise/index.js",
    "start": "npm-run-all -p watch-css server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hansonrb/typecode-test.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hansonrb/typecode-test/issues"
  },
  "homepage": "https://github.com/hansonrb/typecode-test#readme",
  "devDependencies": {
    "http-server": "^0.11.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "ejs": "^2.5.7",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "sequelize": "^4.32.2"
  }
}
